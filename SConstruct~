#!/usr/bin/python

import os

VariantDir('build', 'src', duplicate=0)

moc = Builder(action = 'moc <$SOURCE >$TARGET')
env = Environment(ENV=os.environ, BUILDERS = {'Moc': moc})
Tool('mingw')(env)

env.Moc('build/lines.cpp.moc.h', 'build/lines.cpp')
env.Append(CPPPATH = ['C:/Qt/4.7.3/include', 'build'])
env.Program(['build/lines.cpp'], LIBS = ['QtGui4', 'QtCore4'], LIBPATH = 'C:/Qt/4.7.3/lib')

#env.Program(['build/main.cpp'])

